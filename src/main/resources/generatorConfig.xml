<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>   
    <classPathEntry location="/Users/mac-book/m2/repository/mysql/mysql-connector-java/5.1.35/mysql-connector-java-5.1.35.jar"/>
    <!--<classPathEntry location="H:\\repo\\ojdbc\\ojdbc-11g\\14\\ojdbc-11g-14.jar"/>-->
    <context id="lha" targetRuntime="MyBatis3">

        <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin"></plugin>
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"></plugin>
        <plugin type="org.mybatis.generator.plugins.ToStringPlugin"></plugin>


        <commentGenerator>
            <!-- 这个元素用来去除指定生成的注释中是否包含生成的日期 false:表示保护 -->
            <!-- 如果生成日期，会造成即使修改一个字段，整个实体类所有属性都会发生变化，不利于版本控制，所以设置为true -->
            <property name="suppressDate" value="false"/>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <!--数据库链接URL，用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://tjwqstaging-hdd.doris.srv:10000/newretail"
        				userId="xm_report_wr"
                        password="xm_report_wr"/>
        <javaTypeResolver>
            <!-- This property is used to specify whether MyBatis Generator should force 
                the use of java.math.BigDecimal for DECIMAL and NUMERIC fields, -->
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>
        <!-- 生成模型的包名和位置 -->
        <javaModelGenerator targetPackage="com.xiaomi.cnzone.proretail.dao.model"
                            targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!-- 生成映射文件的包名和位置 -->
        <sqlMapGenerator targetPackage="dbmap"
                         targetProject="src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <!-- 生成DAO的包名和位置 -->
        <javaClientGenerator targetPackage="com.xiaomi.cnzone.proretail.dao"
                             targetProject="src/main/java" type="XMLMAPPER">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>
        


        <!-- 要生成哪些表 -->
        <!--
        <table tableName="ACCOUNT_JPUSH" domainObjectName="AccountJpush" enableCountByExample="false"
               enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
            <generatedKey column="id" sqlStatement="Mysql"/>
        </table>
        -->
        
        
        <table tableName="app_offline_sales_lst_store_goods_deposit_day" domainObjectName="OfflineSalesLstStoreGoodsDepositDay" enableCountByExample="false"
               enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
            <generatedKey column="store_id" sqlStatement="JDBC" identity="true"/>
        </table>
        <table tableName="app_offline_rebate_lst_store_goods_day" domainObjectName="OfflineRebateLstStoreGoodsDay" enableCountByExample="false"
               enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
            <generatedKey column="goods_id" sqlStatement="JDBC" identity="true"/>
        </table>
        <table tableName="dwm_upc_rebate_store_cost_entry_export" domainObjectName="UpcRebateStoreCostEntryExport" enableCountByExample="false"
               enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
            <generatedKey column="id" sqlStatement="JDBC" identity="true"/>
        </table>
        <table tableName="dim_upc_company" domainObjectName="UpcCompany" enableCountByExample="false"
               enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
            <generatedKey column="company_id" sqlStatement="JDBC" identity="true"/>
        </table>
        <table tableName="dim_upc_organization" domainObjectName="UpcOrganization" enableCountByExample="false"
               enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
            <generatedKey column="org_code" sqlStatement="JDBC" identity="true"/>
        </table>

        
    </context>
</generatorConfiguration>